
  <span><% @tweets.each do |tweet| %></span>
  <div class = tweet>
      <div class =write >
        <p><%= tweet.user.nickname%>---<%= tweet.created_at%></p>
        <p><%= tweet.text %>---<%= link_to 'コメント', "/tweets/#{tweet.id}", method: :get %></p>
    </div>

           <% if user_signed_in? && current_user.id == tweet.user_id %>
              <div class = menu>
               <ul>
                <li>
                   <%= link_to '削除', "/tweets/#{tweet.id}", method: :delete %>
                </li>
                <li>
                  <%= link_to '編集', "/tweets/#{tweet.id}/edit", method: :get %>
                </li>
              </ul>
              </div>
           <% end %>
    </div>
  <span><% end %></span>
  <%= paginate(@tweets) %>
  

  




